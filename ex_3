#ex 3 , Grinik Aleksandra
import statistics as stat  # import statistics for further actions
n_list = []  # create list for numbers

n, k = map(int, input().split())  # input the numbers
for i in range(n):  # We enter exactly n numbers
    n_list.append(int(input()))  # input numbers into list

k_list = n_list[:k]  # Select the first k elements
k_list.sort(reverse=False)  #We put the numbers in ascending order


print(f"Arithmetic mean: {stat.mean(k_list)}")  #Calculate the arithmetic mean
print(f"Harmonic Mean: {stat.harmonic_mean(k_list)}")  #Calculate the harmonic mean
print(f"Maximum element: {max(k_list)}")  #Calculating the Maximum element
print(f"The second maximum element: {k_list[-2]}")  # We find the second maximal element using a slice
print(f"Minimum element: {min(k_list)}")  #Calculating the Minimum element
print(f"The second minimum element: {k_list[1]}")  # We find the second minimal element using a slice
